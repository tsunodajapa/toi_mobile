@import "variables"

@font-face
  font-family 'icontoi'
  src url(icomoon-font-path + '/icontoi.eot?iflznm')
  src url(icomoon-font-path + '/icontoi.eot?iflznm#iefix') format('embedded-opentype'), url(icomoon-font-path + '/icontoi.ttf?iflznm') format('truetype'), url(icomoon-font-path + '/icontoi.woff?iflznm') format('woff'), url(icomoon-font-path + '/icontoi.svg?iflznm#icontoi') format('svg')
  font-weight normal
  font-style normal
  font-display block

i
  font-family 'icontoi' !important
  speak never
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-trash
  &:before
    content icon-trash
.icon-received
  &:before
    content icon-received
.icon-sended
  &:before
    content icon-sended
.icon-profile
  &:before
    content icon-profile
.icon-search
  &:before
    content icon-search
.icon-save
  &:before
    content icon-save
.icon-resend
  &:before
    content icon-resend
.icon-send
  &:before
    content icon-send

